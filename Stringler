String[] testExpressions = {
            "()",         // Dengeli
            "(]",         // Dengesiz
            "({[]})",     // Dengeli
            "({[)]}",     // Dengesiz, yazdığım kod bunu dengeli olarak kabul ediyo sayarak gittiğim için onu düzeltmeliyim
            "",           // Dengeli (boş metin)
            "((()))",     // Dengeli
            "(()"         // Dengesiz
        };

        // Her ifadeyi test ediyor ve sonucu yazdırıyoruz.
        for (String expression : testExpressions) {
            ParenthesisChecker checker = new ParenthesisChecker(expression);
            System.out.println("Expression: " + expression + " is " + (checker.isBalanced() ? "Balanced" : "Not Balanced"));
